{
  "manifest_version": 2,

  "name": "ClickBake",
  "description": "Order all the food for the recipe you are looking out and have it shipped to your house",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "order.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://371-1-d-prod.csse.rose-hulman.edu/html/*",
    "cookies"
  ]
}